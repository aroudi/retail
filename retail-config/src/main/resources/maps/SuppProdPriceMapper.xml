        FROM
        SUPP_ORGU_LINK sol
        INNER JOIN
        SUPP_PROD_PRICE spp on (sol.SOL_ID = spp.SOL_ID)
        WHERE
        sol.ORGU_ID = #{param1}
        AND spp.PROD_ID= #{param2}
        AND spp.SPRC_PREFER_BUY = 1 AND spp.SUPPLIER_DELETED = 0
    </select>

    <select id="getAllSuppProdPricesByOrguIdAndProdId" resultMap="suppProdPriceeMap" >
